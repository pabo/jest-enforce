    "util.promisify": "^1.0.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.12.1",
    "eslint-config-recommended": "^1.5.0",
